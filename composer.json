{
    "name": "galo4kin/alohasms-ru",
    "description": "AlohaSmsRu Notifications channel for Laravel 5.3.",
    "keywords": ["laravel", "notifications", "alohasms"],
    "homepage": "https://github.com/galo4kin/alohasms-ru",
    "license": "MIT",
    "authors": [
        {
            "name": "galo4kin",
            "role": "Developer",
            "email": "galo4kin@gmail.com",
            "homepage": "https://github.com/galo4kin"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AlohaSmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AlohaSmsRu\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\AlohaSmsRu\\AlohaSmsRuServiceProvider"
            ]
        }
    }
}
